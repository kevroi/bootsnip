{
    "name": "bootsnip",
    "displayName": "bootsnip",
    "description": "Bootstrap snippet buddy",
    "version": "1.0.0",
    "publisher": "kevroi",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
